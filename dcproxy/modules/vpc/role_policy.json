{
"Version": "2012-10-17",
"Statement": [
{
  "Action": [
      "logs:CreateLogGroup",
      "logs:CreateLogStream",
      "logs:DeleteLogStream",
      "logs:DescribeLogGroups",
      "logs:PutLogEvents"
  ],
    "Effect": "Allow",
    "Resource": "arn:aws:logs:*:*:log-group:${arn_log_group_name}:log-stream:*"
    }
  ]
}
